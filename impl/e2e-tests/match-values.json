{
  "$comment": "use different advertiser sites to avoid depending on budgeting",
  "events": [
    {
      "seconds": 1,
      "site": "publisher.example",
      "event": "saveImpression",
      "$comment": "matchValue defaults to 0",
      "options": { "histogramIndex": 1 }
    },
    {
      "seconds": 2,
      "site": "publisher.example",
      "event": "saveImpression",
      "options": {
        "histogramIndex": 2,
        "matchValue": 100
      }
    },
    {
      "seconds": 3,
      "site": "advertiser-1.example",
      "event": "measureConversion",
      "$comment": "try to select 2 impressions to avoid depending on ordering",
      "options": {
        "aggregationService": "https://agg-service.example",
        "histogramSize": 3,
        "matchValues": [0],
        "value": 2,
        "maxValue": 2,
        "credit": [0.5, 0.5]
      },
      "expected": [0, 2, 0]
    },
    {
      "seconds": 4,
      "site": "advertiser-2.example",
      "event": "measureConversion",
      "$comment": "try to select 2 impressions to avoid depending on ordering",
      "options": {
        "aggregationService": "https://agg-service.example",
        "histogramSize": 3,
        "matchValues": [100],
        "value": 2,
        "maxValue": 2,
        "credit": [0.5, 0.5]
      },
      "expected": [0, 0, 2]
    },
    {
      "seconds": 5,
      "site": "advertiser-3.example",
      "event": "measureConversion",
      "options": {
        "aggregationService": "https://agg-service.example",
        "histogramSize": 3,
        "matchValues": [0, 100],
        "value": 2,
        "maxValue": 2,
        "credit": [0.5, 0.5]
      },
      "expected": [0, 1, 1]
    }
  ]
}
